{
  "plugins": [
    "stylelint-no-unsupported-browser-features"
  ],
  "rules": {
    "plugin/no-unsupported-browser-features": [true, {
      "browsers": ["chrome > 46"],
      "ignore": ["mdn-text-decoration-shorthand", "css-focus-visible", "css-variables", "css-nesting"],
      "ignorePartialSupport": true,
      "severity": "warning"
    }]
  }
}